    "velocity-react": "^1.2.1"
  },
  "engines": {
    "node": "~7.7"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && scripts/build-es5.sh",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts|es5)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/config/jest/__mocks__/styleMock.js"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "react-css-modules",
        {
          "filetypes": {
            ".scss": "postcss-scss"
          },
          "exclude": "node_modules",
          "searchPaths": [
            "src"
          ],
          "webpackHotModuleReloading": true
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "data-stylename"
          ]
        ]
      }
    },
    "ignore": [
      "scripts/templates/*"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "afterAll",
      "afterEach",
      "beforeAll",
      "beforeEach",
      "describe",
      "expect",
      "it",
      "jest"
    ]
  },
  "proxy": "http://localhost:3001"
}
